package com.Capgemini.Date;

public class MyDate
{
	private int date;
	private int month;
	private int year;
	
	
	public void setStartDate(int date,int month,int year)
	{
		this.date=date;
		this.month=month;
		this.year=year;
	}
	public void printStartDate()
	{
		System.out.println(date+"/"+month+"/"+year);
	}
	public void setEndDate(int date,int month,int year)
	{
		this.date=date;
		this.month=month;
		this.year=year;
	}
	public void printEndDate()
	{
		System.out.println(date+"/"+month+"/"+year);
	}
}
