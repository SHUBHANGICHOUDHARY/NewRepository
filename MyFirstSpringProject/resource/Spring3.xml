<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING/DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>  
  
<bean id="triangle"  class="com.shubhangi.autoWiring.Triangle" autowire="byName">
</bean>

	<bean id="pointA"  class="com.shubhangi.autoWiring.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>
	<bean id="pointB" class="com.shubhangi.autoWiring.Point">
			<property name="x" value="-50"></property>
			<property name="y" value="20"></property>
	</bean> 
	<bean id="pointC"  class="com.shubhangi.autoWiring.Point">
		<property name="x" value="20"></property>
		<property name="y" value="40"></property>
	</bean>
  
</beans>  